{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "strict": false,
    "noImplicitAny": false,
    "allowJs": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "resolveJsonModule": true,
//    "strictNullChecks": false,
    "baseUrl": ".",
    "types": [
      "webpack-env",
      "jest",
      "dotenv"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "files": ["./typings.d.ts"]
}
